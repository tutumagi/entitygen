    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch one type",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "entitygen/domain.DeskXXX"
            ],
            "env": {
                "GOFILE": "mapping.go",
                "GOPACKAGE": "entitydef"
            },
            "cwd": "${workspaceFolder}/entitydef"
        },
        {
            "name": "Launch all types",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "entitygen/domain"
            ],
            "env": {
                "GOFILE": "mapping.go",
                "GOPACKAGE": "entitydef"
            },
            "cwd": "${workspaceFolder}/entitydef"
        }
    ]
}